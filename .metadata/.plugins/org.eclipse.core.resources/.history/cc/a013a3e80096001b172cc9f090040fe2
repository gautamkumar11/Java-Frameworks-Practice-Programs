package in.sts.springbootrest.services;

import java.util.ArrayList;
import java.util.List;

import in.sts.springbootrest.entities.Course;

public class CourseServiceImpl implements CourseService {

	List<Course> list;
	
	public CourseServiceImpl() {
		list = new ArrayList<>();
		list.add(new Course(145,"Java Core Courses", "This Course is for java learner"));
		list.add(new Course(146,"Spring boot Courses", "This Course is for spring boot learner"));
	}

	@Override
	public List<Course> getCourses() {
		
		return list;
	}

}
