package in.sts.springbootrest.entities;

public class Course {
	
	private int id;
	private String title;
	private String description;
	
	public Course(int id, String title, String description) {
		super();
		this.id = id;
		this.title = title;
		this.description = description;
	}

	public Course() {
		super();
		
	}
	
	
}
